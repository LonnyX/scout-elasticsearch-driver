{
    "name": "babenkoivan/scout-elasticsearch-driver",
    "type": "library",
    "description": "Elasticsearch driver for Laravel Scout",
    "keywords": [
        "elastic",
        "elasticsearch",
        "driver",
        "engine",
        "laravel",
        "scout",
        "search"
    ],
    "homepage": "https://babenkoivan.github.io/scout-elasticsearch-driver/",
    "license": "MIT",
    "authors": [
        {
            "name": "Babenko Ivan",
            "email": "babenko.i.a@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "babenkoivan/fluent-array": "@beta",
        "laravel/scout": "^5.0",
        "elasticsearch/elasticsearch": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "BabenkoIvan\\ScoutElasticsearchDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabenkoIvan\\ScoutElasticsearchDriver\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BabenkoIvan\\ScoutElasticsearchDriver\\Application\\ServiceProvider"
            ]
        }
    }
}
